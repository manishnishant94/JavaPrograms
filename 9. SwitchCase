package com.nishant;

public class Main {

    public static void main(String[] args) {
	    char character = 'z' ;
	    switch (character) {
            case 'A':
                System.out.println("This is A");
                break;
            case 'B':
                System.out.println("this is B");
                break;
            case 'C':
                System.out.println("This is C");
                break;
            case 'D':case 'E':case 'F':
                System.out.println("This is D or E or F");
                break;
            default:
                System.out.println("other than A B C D E F");
                break;
        }
    }
}
